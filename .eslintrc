{
  "extends": "defaults/configurations/walmart/es6-react",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "func-style": 0,
    "arrow-parens": 0,
    "no-console": 0,
    "no-invalid-this": 0,
    "no-unused-expressions": 0,
    "space-before-function-paren": 0,
    "max-len": [2, 120],
    "react/wrap-multilines": 0,
    "react/sort-comp": 2,
    "filenames/filenames": 0
  }
}
